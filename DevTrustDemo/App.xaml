<Application x:Class="DevTrustDemo.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:diav="clr-namespace:DevTrustDemo.Views.Dialogs"
             xmlns:diavm="clr-namespace:DevTrustDemo.ViewModels.DialogViewModels"
             StartupUri="MainWindow.xaml">
    
    <Application.Resources>
        <diav:InformationDialogView x:Key="{x:Type diavm:InformationDialogViewModel}" x:Shared="False"/>
        <diav:WarningDialogView x:Key="{x:Type diavm:WarningDialogViewModel}" x:Shared="False"/>
    </Application.Resources>
</Application>
